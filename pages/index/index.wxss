.lable{
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  margin: 0 auto;
  width: 670rpx;
}
.teacher-card{
  display: flex;
  width: 134rpx;
  border: 1px solid gray;
  padding: 0px 6rpx 0px 6rpx;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  height: 60rpx;
  font-size: 24rpx;
  margin-top: 20rpx;
  margin-left: 14rpx;
}
.active{
  border-color: orange;
  color: orange;
}
.search{
  display: flex;
  /* border: 1px solid #000000; */
  background-color: #2931;
  width: 90%;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 30rpx;
}
.search-icon{
  width: 30px;
  opacity: 1;
  background-repeat: no-repeat;
  background-position: left center;
  /* padding-left: 30px; */
  background-image: url("data:image/png;base64,");
  margin-left: 10rpx;
}
.search-input{
  opacity: 1;
  font-size: 14px;
  height: 40px;
  width: 100%;
  line-height: 40px;
}
.title{
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin: 0 auto;
  height: 40rpx;
  margin-top: 40rpx;
  font-size: 28rpx;
}
.title text text{
  color: red
}
.selectedView{
  position: absolute;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  height: 600rpx;
  background-color: #fff;
  z-index: 999;
  font-size: 14px;
  padding-left: 20rpx;
}
.selectedView text{
  width: 100%;
  height: 40rpx;
}
.selectedView view{
  width: 100%;
  height: 40rpx;
  margin-left: 40rpx;
}
.selectedView text text{
  color: red;
}
.cover{
  position: absolute;
  width: 100%;
  height: 1334rpx;
  background-color: gray;
  opacity: 0.8;
  z-index: 998;
}
.selectActive{
  color: red
}