.lable{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 0 auto;
  width: 90%;
}
.teacher-card{
  display: flex;
  width: 70px;
  border: 1px solid gray;
  padding: 0px 3px 0px 3px;
  align-items: center;
  justify-content: center;
  border-radius: 5px;
  height: 30px;
  font-size: 12px;
  margin-top: 20px;
}
.active{
  border-color: orange;
  color: orange;
}
.search{
  display: flex;
  /* border: 1px solid #000000; */
  background-color: #2931;
  width: 90%;
  margin: 0 auto;
  margin-top: 10px;
  border-radius: 15px;
}
.search-icon{
  width: 30px;
  height: 30px;
  opacity: 1;
  background-repeat: no-repeat;
  background-position: left center;
  /* padding-left: 30px; */
  background-image: url("data:image/png;base64,");
  margin-left: 5px;
}
.search-input{
  opacity: 1;
  font-size: 14px;
  height: 30px;
  line-height: 30px;
}
.title{
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin: 0 auto;
  height: 20px;
  margin-top: 20px;
  font-size: 14px;
}